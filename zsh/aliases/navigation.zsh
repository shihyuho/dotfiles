#!/usr/bin/env zsh
# ---
# Navigation aliases
# Updated: 2026-03-02
# ---

# Easier navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"
alias -- -="cd -"

# Common directories
alias dl="cd $HOME/Downloads"
alias doc="cd $HOME/Documents"
alias cfg="cd $HOME/.config"
alias tmp="cd $HOME/tmp"
